{
  "name": "devbox",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sync-assets": "lorikeet copy-assets ./dist",
    "build": "npm run sync-assets && webpack --mode production",
    "build:dev": "npm run sync-assets && webpack --mode development",
    "update-index": "./scripts/examples-index ../src/components > ./apps.js",
    "start": "npm run sync-assets && webpack-dev-server --mode development --port 1234 --host 0.0.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@lorikeet/ui": "*",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
